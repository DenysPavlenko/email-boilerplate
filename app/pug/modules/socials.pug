mixin socials()
	- var top    = attributes.top ? `padding-top:${attributes.top}px;` : '';
	- var bottom = attributes.bottom ? `padding-bottom:${attributes.bottom}px;` : '';

	//
		socials
	tr(class=attributes.mob)
		td(valign="top" align=`${attributes.align||'left'}` style=`${top}${bottom}${attributes.style||''}`)
			table(align=`${attributes.align||'left'}` border="0" cellpadding="0" cellspacing="0")
				tr
					each padding, icon in attributes.icons
						th(style=`padding:${padding}` data-repeatable)
							img(src=icon border="0" width="auto" height="auto" alt="" style="display:block; width: auto;")
	//
		socials